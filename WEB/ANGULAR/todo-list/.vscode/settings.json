{
    "cSpell.ignoreWords": [
        "list",
        "stack",
        "task"
    ],
    "files.autoSave": "afterDelay",
    "files.defaultLanguage": "ru-ru",
    "files.exclude": {
        "**/.exe": true,
        "**/.img": true,
        "**/.rar": true,
        "**/.zip": true
    },
    "files.insertFinalNewline": true,
    "files.maxMemoryForLargeFilesMB": 2048,
    "files.simpleDialog.enable": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.history": 100,
    "workbench.enableExperiments": false,
    "workbench.list.multiSelectModifier": "alt",
    "workbench.quickOpen.preserveInput": true,
    "workbench.colorTheme": "Visual Studio Light",
    "workbench.preferredDarkColorTheme": "Visual Studio Dark",
    "workbench.preferredLightColorTheme": "Visual Studio Light",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 5,
    "workbench.editor.splitSizing": "split",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.titleScrollbarSizing": "large",
    "window.autoDetectColorScheme": true,
    "window.titleSeparator": " = ",
    "window.zoomLevel": 0,
    "explorer.confirmDragAndDrop": false,
    "explorer.enableDragAndDrop": false,
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 5,
    "search.collapseResults": "auto",
    "search.quickOpen.history.filterSortOrder": "recency",
    "search.searchEditor.doubleClickBehaviour": "selectWord",
    "search.searchOnTypeDebouncePeriod": 500,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "debug.console.closeOnEnd": true,
    "debug.console.fontSize": 12,
    "debug.inlineValues": true,
    "debug.onTaskErrors": "debugAnyway",
    "scm.defaultViewMode": "tree",
    "scm.diffDecorations": "gutter",
    "extensions.showRecommendationsOnlyOnDemand": true,
    "extensions.ignoreRecommendations": true,
    "terminal.integrated.allowChords": false,
    "terminal.integrated.detectLocale": "on",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeightBold": "normal",
    "comments.openPanel": "neverOpen",
    "keyboard.dispatch": "keyCode",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "cSpell.language": "ru-ru",
    "json.maxItemsComputed": 1000,
    "npm.enableRunFromFolder": true,
    "npm-intellisense.scanDevDependencies": true,
    "path-intellisense.autoSlashAfterDirectory": true,
    "php.validate.enable": false,
    "php.suggest.basic": false,
    "sqlite.logLevel": "WARN",
    "sqlite.recordsPerPage": 100,
    "tslint.alwaysShowStatus": true,
    "tslint.jsEnable": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "javascript.format.semicolons": "remove",
    "javascript.preferences.importModuleSpecifierEnding": "minimal",
    "javascript.preferences.quoteStyle": "single",
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.suggest.names": false,
    "typescript.format.semicolons": "insert",
    "typescript.locale": "ru",
    "typescript.preferences.importModuleSpecifierEnding": "minimal",
    "typescript.preferences.quoteStyle": "single",
    "typescript.referencesCodeLens.enabled": true,
    "typescript.referencesCodeLens.showOnAllFunctions": true,
    "typescript.suggest.completeFunctionCalls": true,
    "typescript.tsc.autoDetect": "off",
    "typescript.tsserver.maxTsServerMemory": 2048,
    "editor.fontSize": 12,
    "markdown.preview.fontSize": 12,
    "markdown.preview.lineHeight": 1.1,
    "terminal.integrated.lineHeight": 1.1,
    "cSpell.words": [
        "crud",
        "i",
        "item",
        "subscribe"
    ]
}